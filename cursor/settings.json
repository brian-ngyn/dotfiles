{
  "vim.leader": "<space>",
  "vim.surround": true,
  "vim.camelCaseMotion.enable": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.navigateLeft"],
      "silent": true
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.navigateRight"],
      "silent": true
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Leader>", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "/"],
      "after": ["g", "c", "c"]
    },
    {
      "before": ["<leader>", "l", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["g", "l"],
      "commands": ["editor.action.showHover"],
      "silent": true
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.navigateLeft"],
      // "when": "explorerViewletVisible && filesExplorerFocus",
      "silent": true
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.navigateRight"],
      // "when": "explorerViewletVisible && filesExplorerFocus",
      "silent": true
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"],
      "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly",
      "silent": true
    },
    {
      "before": ["<C-y>"],
      "commands": ["editor.gotoNextFold"],
      "silent": true
    },
    {
      "before": ["<C-o>"],
      "commands": ["editor.gotoPreviousFold"],
      "silent": true
    },
    {
      "before": ["g", "b"],
      "commands": ["workbench.action.navigateBack"],
      "silent": true
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "/"],
      "after": ["g", "c"]
    },
    {
      "before": ["<M-j>"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["tab"],
      "commands": ["editor.action.indentLines"],
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "before": ["shift+tab"],
      "commands": ["editor.action.outdentLines"],
      "when": "editorTextFocus && !editorReadonly"
    }
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "prettier.requireConfig": false,
  "editor.accessibilitySupport": "off",
  "git.openRepositoryInParentFolders": "always",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "FiraCode Nerd Font Propa",
  "editor.fontSize": 12.2,
  "editor.fontLigatures": true,
  "security.workspace.trust.untrustedFiles": "open",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.smoothScrolling": true,
  "explorer.sortOrder": "type",
  "editor.lineNumbers": "relative",
  "editor.tabSize": 2,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 13,
  "workbench.colorCustomizations": {
    "terminal.foreground": "#839496",
    "terminal.ansiBlack": "#073642",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiBrightBlack": "#002b36",
    "terminal.ansiBrightBlue": "#839496",
    "terminal.ansiBrightCyan": "#93a1a1",
    "terminal.ansiBrightGreen": "#586e75",
    "terminal.ansiBrightMagenta": "#6c71c4",
    "terminal.ansiBrightRed": "#dc322f",
    "terminal.ansiBrightWhite": "#fdf6e3",
    "terminal.ansiBrightYellow": "#657b83",
    "terminal.ansiCyan": "#2aa198",
    "terminal.ansiGreen": "#859900",
    "terminal.ansiMagenta": "#d33682",
    "terminal.ansiRed": "#dc322f",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiYellow": "#e8b105",
    "terminal.selectionBackground": "#073642",
    "terminalCursor.foreground": "#839496"
  },
  "terminal.integrated.cursorBlinking": true,
  "editor.cursorStyle": "line",
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.autoSave": "afterDelay",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.minimap.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "editor.scrollBeyondLastLine": false,
  "editor.padding.bottom": 200,
  "editor.cursorSurroundingLines": 12,
  "editor.renderLineHighlight": "all",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.enableSmartCommit": true,
  "editor.largeFileOptimizations": false,
  "security.promptForLocalFileProtocolHandling": false,
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prisma.showPrismaDataPlatformNotification": false,
  "remote.SSH.connectTimeout": 300,
  "prettier.trailingComma": "all",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontLigatures.enabled": true,
  "editor.fontVariations": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.css": "css"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "terminal.integrated.scrollback": 10000,
  "workbench.sideBar.location": "right",
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
  "cursor.composer.usageSummaryDisplay": "always",
  "cursor.composer.queueMessageDefaultBehavior": "queue",
  "cursor.cpp.enablePartialAccepts": true,
  "cursor.general.disableHttp2": true
}
