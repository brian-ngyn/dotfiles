{
  "wsl_connections": [
    {
      "distro_name": "archlinux",
      "user": null,
      "projects": [
        {
          "paths": ["/root"]
        }
      ]
    }
  ],
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "vim_mode": true,
  "base_keymap": "Cursor",
  "inlay_hints": {
    "enabled": false,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true,
    "show_background": true,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50,
    "toggle_on_modifiers_press": {
      "function": true
    }
  },
  "context_servers": {},
  "remove_trailing_whitespace_on_save": false,
  "ui_font_weight": 500.0,
  "edit_predictions": {
    "disabled_globs": []
  },
  "git_panel": {
    "status_style": "icon",
    "dock": "right",
    "sort_by_path": true
  },
  "project_panel": {
    "dock": "right"
  },
  "toolbar": {
    "code_actions": true
  },
  "hide_mouse": "never",
  "restore_on_startup": "last_workspace",
  "use_system_prompts": true,
  "use_system_path_prompts": true,
  "ui_font_family": ".ZedSans",
  "buffer_line_height": "comfortable",
  "buffer_font_weight": 500.0,
  "agent": {
    "default_profile": "write",
    "always_allow_tool_actions": true,
    "inline_assistant_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4.5"
    },
    "default_model": {
      "provider": "copilot_chat",
      "model": "grok-code-fast-1"
    },
    "dock": "left",
    "model_parameters": []
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "agent_servers": {
    "your_agent": {
      "type": "custom",
      "command": "path_to_executable",
      "args": [],
      "env": {}
    }
  },
  "terminal": {
    "env": {
      "Q_NEW_SESSION": "\"1\""
    },
    "font_size": 13.0,
    "font_family": "FiraCode Nerd Font Propo",
    "blinking": "on",
    "max_scroll_history_lines": 10000
  },
  "relative_line_numbers": "enabled",
  "vertical_scroll_margin": 12.0,
  "gutter": {
    "line_numbers": true
  },
  "minimap": {
    "display_in": "active_editor",
    "show": "never"
  },
  "show_completions_on_input": true,
  "current_line_highlight": "all",
  "cursor_shape": "bar",
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "buffer_font_family": "FiraCode Nerd Font Propo",
  "file_types": {
    "css": ["*.css"]
  },
  "show_edit_predictions": true,
  "tab_size": 2,
  "icon_theme": "Material Icon Theme",
  "ui_font_size": 15,
  "buffer_font_size": 12.4,
  "theme": "One Dark Pro",
  "format_on_save": "on",
  "formatter": "prettier",
  "lsp_document_colors": "background",
  "linked_edits": true,
  "languages": {
    "Go": {
      "formatter": "language_server"
    },
    "Scala": {
      "formatter": "language_server"
    },
    "JavaScript": {
      "language_servers": ["typescript-language-server", "eslint"]
    },
    "TypeScript": {
      "language_servers": ["typescript-language-server", "eslint"]
    },
    "TSX": {
      "language_servers": ["typescript-language-server", "eslint"]
    }
  },
  "lsp": {
    "biome": {
      "settings": {
        "require_config_file": true
      }
    }
  }
}
